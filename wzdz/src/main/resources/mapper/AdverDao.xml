<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.wzdz.dao.AdverDao">

	
	<select id="findByAll"  resultType="com.wzdz.entity.Adver"> 
		SELECT
		t.id,
		t.gg_file
		FROM wzdz_tb_adver t
		order by t.create_date Desc
	</select>
	
</mapper>