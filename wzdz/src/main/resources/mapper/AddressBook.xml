<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.wzdz.dao.AddressBookDao">
	
	<select id="findAll"  resultType="com.wzdz.entity.AddressBook"> 
		SELECT
		t.id,
		t.create_date,
		t.update_date,
		t.login_id,
		t.name,
		t.tel
		FROM wzdz_tb_address_book t
	</select>
	
</mapper>