<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wzdz.dao.YjfkDao">
	
	<insert id="insertIntoYjfk" parameterType="com.wzdz.entity.Yjfk">
		insert 
		into 
		wzdz_tb_yjfk (create_date,update_date,content,logo,name,phone)
		values(now(),now(),#{content},#{logo},#{name},#{phone})
		
	</insert>
</mapper>